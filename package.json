{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sunbeam",
  "title": "Sunbeam",
  "description": "Run Sunbeam Extension from Raycast",
  "icon": "command-icon.png",
  "author": "pomdtr",
  "license": "MIT",
  "commands": [
    {
      "name": "fetch-page",
      "title": "Fetch Page",
      "description": "Fetch Page from url",
      "mode": "view",
      "arguments": [
        {
          "name": "url",
          "type": "text",
          "placeholder": "url",
          "required": false
        }
      ]
    },
    {
      "name": "eval-expression",
      "title": "Eval Expression",
      "description": "Eval Expression in val.town",
      "mode": "view",
      "preferences": [
        {
          "name": "valTownToken",
          "title": "Val Town Token",
          "description": "Token for val.town",
          "type": "password",
          "required": true
        }
      ],
      "arguments": [
        {
          "name": "expression",
          "type": "text",
          "placeholder": "expression",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.0",
    "cross-fetch": "^3.1.6"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/which": "^3.0.0",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "sunbeam-types": "^0.17.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
