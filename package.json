{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sunbeam",
  "title": "Sunbeam",
  "description": "Run Sunbeam Extension from Raycast",
  "icon": "command-icon.png",
  "author": "pomdtr",
  "license": "MIT",
  "commands": [
    {
      "name": "run-command",
      "title": "Run Sunbeam Command",
      "description": "Run Sunbeam Command",
      "mode": "view",
      "arguments": [
        {
          "name": "command",
          "type": "text",
          "placeholder": "command",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.0",
    "execa": "^7.1.1",
    "shlex": "^2.1.2",
    "which": "^3.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/which": "^3.0.0",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "sunbeam-types": "^0.8.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
